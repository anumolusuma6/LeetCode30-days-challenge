SELECT
    c.contest_id,
    ROUND(c.user_count / (SELECT COUNT(user_id) FROM Users) * 100, 2) AS percentage
FROM
    (SELECT
        b.contest_id,
        COUNT(b.user_id) AS user_count
    FROM
        Register b
    INNER JOIN
        Users a ON a.user_id = b.user_id
    GROUP BY
        b.contest_id) c
order by percentage desc, contest_id asc

--or

select contest_id, round((count(*)/(select count(user_name) from Users))*100,2) as percentage from
Register
group by contest_id
order by percentage DESC, contest_id ASC ;

--or

