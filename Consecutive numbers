select distinct l1.num as ConsecutiveNums
from logs l1
     ,logs l2
     ,logs l3
where l2.num = l1.num and l2.id=l1.id+1
and   l3.num = l1.num and l3.id=l1.id+2;

--or

select distinct l1.num as ConsecutiveNums
from logs l1
     ,logs l2
     ,logs l3
where l2.num = l1.num and l1.id=l2.id-1
and   l3.num = l1.num and l2.id=l3.id-1;

--or
select distinct a.num as ConsecutiveNums from logs a join logs b on b.id=a.id+1 and a.num=b.num join 
logs c on c.id=a.id+2 and a.num=c.num;
